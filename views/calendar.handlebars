<div class="modal fade" id="exampleModal3" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Update Profile</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form action="/editProfile/{{uName}}" method="POST">
                    <div class="form-group d-none">
                        <div class="input-group input-group-lg">
                            <div class="input-group-prepend">
                                <span class="input-group-text bg-dark text-light">
                                    <i class="fas fa-heading"></i>
                                </span>
                            </div>
                            <input type="text" id="id" name="id" class="form-control bg-light text-black updateNumber">
                        </div>
                    </div>
                    <label for="profileAboutMe">About Me:</label>
                    <div class="form-group">
                        <div class="input-group input-group-lg">
                            <div class="input-group-prepend">
                                <span class="input-group-text bg-dark text-light">
                                    <i class="fas fa-heading"></i>
                                </span>
                            </div>
                            {{#if missing_about_me}}
                            <textarea class="form-control editProfInfo" name="profileAboutMe" id="profileAboutMe"
                                rows="5"> Please enter a brief description of yourself here. </textarea>
                            {{else}}
                            <textarea class="form-control editProfInfo" name="profileAboutMe" id="profileAboutMe"
                                rows="5">{{info.about_me}}</textarea>
                            {{/if}}
                        </div>
                    </div>
                    <label for="profileInterests">Interests:</label>
                    <div class="form-group">
                        <div class="input-group input-group-lg">
                            <div class="input-group-prepend">
                                <span class="input-group-text bg-dark text-light">
                                    <i class="fas fa-heading"></i>
                                </span>
                            </div>

                            {{#if missing_interests}}
                            <textarea class="form-control editProfInfo" name="profileInterests" id="profileInterests"
                                rows="3">Please list some of your interests here.</textarea>
                            {{else}}
                            <textarea class="form-control editProfInfo" name="profileInterests" id="profileInterests"
                                rows="3">{{info.interests}}</textarea>
                            {{/if}}
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="coverPhoto">Cover Photo URL</label>
                        <input type="text" class="form-control" id="coverPhotoURL" name="coverPhotoURL"
                            placeholder="https://imgur.com/upload" value="{{info.cover_photo_URL}}">
                    </div>


                    <input type="submit" value="Update"
                        class="btn bg-dark text-light btn-block btn-lg m-0 submit-btn" />
                </form>
            </div>
            <div class="modal-footer">

            </div>
        </div>
    </div>
</div>

