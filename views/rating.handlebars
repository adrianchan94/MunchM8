<div class="secondPageBanner">
    <div class="mainNav">
        <nav class="navbar navbar-expand-lg navbar-dark">
            <a class="mainLogo" href="/main/{{uName}}"><img class="mainLogo" src="/images/logo.png" width="200"></a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
                aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item">
                        <a class="nav-link" href="/main/{{uName}}">Find A Table<span
                                class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/secondary/{{uName}}">Profile</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/my_tables/{{uName}}">My Tables</a>
                    </li>
                </ul>
            </div>
        </nav>
    </div>
    <div class="container secPageHeadContainer">
        <div class="row">
            <div class="col-12">
                <h1 class="secPageHeader">My Munch Profile</h1>
            </div>
        </div>
    </div>
</div>
<div class="secPageMain">
    <div class="row">

        <div class="col-lg-6 col-md-6">
            <div class="cardDeckFrame">
                <div class="card-deck">
                    <div class="card cardFrame bg-secondary">
                        <div class="card-header bg-dark text-white border-success">
                            <h1 class="profileHeader text-center card-title table-card-header text-white">
                                {{filter.restaurant_name}}</h1>
                        </div>
                        <div class="cardInfo">

                            <ul class="list-group text-dark col-12">
                                <li class="list-group-item address list-group-item-dark"><b>Host Name:</b>
                                    {{filter.host_name}}</li>
                            </ul>
                            <ul class="list-group text-dark col-12">
                                <li class="list-group-item address list-group-item-dark"><b>Number of Guests:</b>
                                    {{filter.number_of_guests}} out of {{filter.max_number_guests}}</li>
                            </ul>
                            <ul class="list-group text-dark col-12">
                                <li class="list-group-item address list-group-item-dark"><b>Preferred Language:</b>
                                    {{filter.preferred_language}}</li>
                            </ul>
                            <ul class="list-group text-dark col-12">
                                <li class="list-group-item address list-group-item-dark"><b>Date and Time:</b>
                                    {{filter.date_and_time}}</li>
                            </ul>
                            <ul class="list-group text-dark col-12">
                                <li class="list-group-item description list-group-item-dark"><b>Description:</b>
                                    {{filter.description}}</li>

                            </ul>
                            <ul class="list-group text-dark col-12">
                                <li class="list-group-item address list-group-item-dark"><b>Address:</b>
                                    {{filter.restaurant_address}}</li>
                            </ul>

                        </div>
                    </div>
                </div>
            </div>

        </div>


        <div class="col-lg-6 col-md-6">
            <div class="cardDeckFrame">
                <div class="card-deck">
                    <div class="card cardFrame bg-secondary">
                        <div class="text-white card-body">
                            {{#if host}}
                            <h5 class="card-title card-header bg-dark text-white confirmedParHead"> {{variable.host_name}} Rating</h5>
                            <div class="cardInfo bg-secondary ratingBox">
                                <div class="my-rating-4"></div>
                            </div>
                            {{/if}}
                            {{#if guest_1}}
                            <h5 class="card-title card-header bg-dark text-white confirmedParHead"> {{variable.guest_1}} Rating</h5>
                            <div class="cardInfo bg-secondary ratingBox">
                                <div class="my-rating-5"></div>
                            </div>
                            {{/if}}
                            {{#if guest_2}}
                            <h5 class="card-title card-header bg-dark text-white confirmedParHead">{{variable.guest_2}} Rating</h5>
                            <div class="cardInfo bg-secondary ratingBox">
                                <div class="my-rating-6"></div>
                            </div>
                            {{/if}}
                            {{#if guest_3}}
                            <h5 class="card-title card-header bg-dark text-white confirmedParHead">{{variable.guest_3}} Rating</h5>
                            <div class="cardInfo bg-secondary ratingBox">
                                <div class="my-rating-7"></div>
                            </div>
                            {{/if}}
                            {{#if guest_4}}
                            <h5 class="card-title card-header bg-dark text-white confirmedParHead">{{variable.guest_4}} Rating</h5>
                            <div class="cardInfo bg-secondary ratingBox">
                                <div class="my-rating-8"></div>
                            </div>
                            {{/if}}
                            {{#if guest_5}}
                            <h5 class="card-title card-header bg-dark text-white confirmedParHead">{{variable.guest_5}} Rating</h5>
                            <div class="cardInfo bg-secondary ratingBox">
                                <div class="my-rating-9"></div>
                            </div>
                            {{/if}}
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>

<footer>
    <div class="footer">
        <p>Â©2020 MunchM8. All Rights Reserved.</p>
    </div>
</footer>

<div class="d-none uName">{{uName}}</div>
<div class="d-none roomName">{{roomId}}</div>

<div class="d-none hostName">{{variable.host_name}}</div>
<div class="d-none g1">{{variable.guest_1}}</div>

<div class="d-none g2">{{variable.guest_2}}</div>
<div class="d-none g3">{{variable.guest_3}}</div>

<div class="d-none g4">{{variable.guest_4}}</div>
<div class="d-none g5">{{variable.guest_5}}</div>


